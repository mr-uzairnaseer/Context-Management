<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="#ffffff">
    <item>
        <shape android:shape="oval">
            <size
                android:height="64dp"
                android:width="64dp"/>
            <solid android:color="#2c2c2c"/>
        </shape>
    </item>
    <item
        android:top="8dp"
        android:bottom="8dp"
        android:start="8dp"
        android:end="8dp">
        <shape android:shape="oval">
            <solid android:color="#000000"/>
        </shape>
    </item>
    <item
        android:gravity="center"
        android:height="32dp"
        android:width="32dp"
        android:drawable="@drawable/baseline_settings_24"/>
</ripple>
