<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@drawable/bg_white_grey_rounded_corners"
    android:paddingLeft="@dimen/default_margin_and_padding"
    android:paddingTop="24dp"
    android:paddingRight="@dimen/default_margin_and_padding"
    android:paddingBottom="24dp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginLeft="12dp"
    android:layout_marginTop="24dp"
    android:layout_marginRight="12dp"
    android:layout_marginBottom="12dp">
    <TextView
        android:textSize="18sp"
        android:textColor="@color/black"
        android:id="@+id/premium_owns_expiring_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/premium_owns_expires_title"
        android:fontFamily="?attr/fontBold"/>
    <TextView
        android:textSize="16sp"
        android:textColor="@color/black"
        android:id="@+id/premium_owns_expiring_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="@string/premium_owns_expires_text"
        android:fontFamily="?attr/fontRegular"/>
    <TextView
        android:textSize="18sp"
        android:textColor="@color/black"
        android:id="@+id/premium_owns_expiring_header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/premium_owns_expires_features"
        android:fontFamily="?attr/fontBold"/>
    <LinearLayout
        android:id="@+id/premium_owns_expiring_feature_mdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp">
        <ImageView
            android:id="@+id/premium_owns_expiring_feature_image"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="0x7f0810c3"/>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginStart="@dimen/default_margin_and_padding">
            <TextView
                android:textSize="16sp"
                android:textColor="@color/black"
                android:id="@+id/premium_owns_expiring_feature_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/premium_owns_expires_live_tracking_title"
                android:fontFamily="?attr/fontBold"/>
            <TextView
                android:textSize="16sp"
                android:textColor="@color/black"
                android:id="@+id/premium_owns_expiring_feature_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:text="@string/premium_owns_expires_live_tracking_text"
                android:fontFamily="?attr/fontRegular"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/premium_owns_expiring_feature_live_tracking"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp">
        <ImageView
            android:id="@+id/premium_owns_expiring_feature_image"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="0x7f081115"/>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginStart="@dimen/default_margin_and_padding">
            <TextView
                android:textSize="16sp"
                android:textColor="@color/black"
                android:id="@+id/premium_owns_expiring_feature_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/premium_owns_expires_mdp_title"
                android:fontFamily="?attr/fontBold"/>
            <TextView
                android:textSize="16sp"
                android:textColor="@color/black"
                android:id="@+id/premium_owns_expiring_feature_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:text="@string/premium_owns_expires_mdp_text"
                android:fontFamily="?attr/fontRegular"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/premium_owns_expiring_feature_pc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp">
        <ImageView
            android:id="@+id/premium_owns_expiring_feature_image"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="0x7f081148"/>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginStart="@dimen/default_margin_and_padding">
            <TextView
                android:textSize="16sp"
                android:textColor="@color/black"
                android:id="@+id/premium_owns_expiring_feature_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/premium_owns_expires_personal_collections_title"
                android:fontFamily="?attr/fontBold"/>
            <TextView
                android:textSize="16sp"
                android:textColor="@color/black"
                android:id="@+id/premium_owns_expiring_feature_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:text="@string/premium_owns_expires_personal_collections_text"
                android:fontFamily="?attr/fontRegular"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/premium_owns_expiring_feature_ssm"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp">
        <ImageView
            android:id="@+id/premium_owns_expiring_feature_image"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="0x7f08117d"/>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginStart="@dimen/default_margin_and_padding">
            <TextView
                android:textSize="16sp"
                android:textColor="@color/black"
                android:id="@+id/premium_owns_expiring_feature_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/premium_owns_expires_ssm_title"
                android:fontFamily="?attr/fontBold"/>
            <TextView
                android:textSize="16sp"
                android:textColor="@color/black"
                android:id="@+id/premium_owns_expiring_feature_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:text="@string/premium_owns_expires_ssm_text"
                android:fontFamily="?attr/fontRegular"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/premium_owns_expiring_feature_weather"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp">
        <ImageView
            android:id="@+id/premium_owns_expiring_feature_image"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:src="0x7f0811a2"/>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginStart="@dimen/default_margin_and_padding">
            <TextView
                android:textSize="16sp"
                android:textColor="@color/black"
                android:id="@+id/premium_owns_expiring_feature_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/premium_owns_expires_weather_title"
                android:fontFamily="?attr/fontBold"/>
            <TextView
                android:textSize="16sp"
                android:textColor="@color/black"
                android:id="@+id/premium_owns_expiring_feature_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:text="@string/premium_owns_expires_weather_text"
                android:fontFamily="?attr/fontRegular"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/premium_owns_expiring_feature_external_maps"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp">
        <ImageView
            android:id="@+id/premium_owns_expiring_feature_image"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="@drawable/maps_on_garmin"/>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginStart="@dimen/default_margin_and_padding">
            <TextView
                android:textSize="16sp"
                android:textColor="@color/black"
                android:id="@+id/premium_owns_expiring_feature_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/premium_owns_external_maps_cta"
                android:fontFamily="?attr/fontBold"/>
            <TextView
                android:textSize="16sp"
                android:textColor="@color/black"
                android:id="@+id/premium_owns_expiring_feature_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:text="@string/buy_premium_compare_external_maps_text"
                android:fontFamily="?attr/fontRegular"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/premium_owns_expiring_feature_offline_maps"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp">
        <ImageView
            android:id="@+id/premium_owns_expiring_feature_image"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="0x7f08112c"/>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginStart="@dimen/default_margin_and_padding">
            <TextView
                android:textSize="16sp"
                android:textColor="@color/black"
                android:id="@+id/premium_owns_expiring_feature_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/premium_owns_expires_offline_maps_title"
                android:fontFamily="?attr/fontBold"/>
            <TextView
                android:textSize="16sp"
                android:textColor="@color/black"
                android:id="@+id/premium_owns_expiring_feature_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:text="@string/premium_owns_expires_offline_maps_text"
                android:fontFamily="?attr/fontRegular"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/premium_owns_expiring_feature_discounts"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp">
        <ImageView
            android:id="@+id/premium_owns_expiring_feature_image"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="0x7f08085e"/>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginStart="@dimen/default_margin_and_padding">
            <TextView
                android:textSize="16sp"
                android:textColor="@color/black"
                android:id="@+id/premium_owns_expiring_feature_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/premium_owns_expires_discounts_title"
                android:fontFamily="?attr/fontBold"/>
            <TextView
                android:textSize="16sp"
                android:textColor="@color/black"
                android:id="@+id/premium_owns_expiring_feature_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:text="@string/premium_owns_expires_discounts_text"
                android:fontFamily="?attr/fontRegular"/>
        </LinearLayout>
    </LinearLayout>
    <Button
        android:textSize="16sp"
        android:textColor="@color/white"
        android:id="@+id/premium_owns_state_btn"
        android:background="@drawable/btn_primary"
        android:layout_width="wrap_content"
        android:layout_height="36dp"
        android:layout_marginTop="16dp"
        android:text="@string/premium_owns_expires_resubscribe"
        android:textAllCaps="true"
        android:fontFamily="?attr/fontRegular"
        android:paddingStart="@dimen/default_margin_and_padding"
        android:paddingEnd="@dimen/default_margin_and_padding"/>
</LinearLayout>
