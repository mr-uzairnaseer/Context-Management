<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:title="@string/settings_dev_settings"
    android:key="pref_key_app_settings">
    <SwitchPreference
        android:persistent="false"
        android:title="@string/settings_dev_logcat_upload"
        android:key="user_pref_logcat_upload"
        app:iconSpaceReserved="false"/>
    <SwitchPreference
        android:persistent="false"
        android:title="@string/setting_switch_debug_picasso"
        android:key="user_pref_debug_picasso"
        app:iconSpaceReserved="false"/>
    <SwitchPreference
        android:persistent="false"
        android:title="@string/setting_switch_debug_gcm"
        android:key="user_pref_debug_fcm"
        app:iconSpaceReserved="false"/>
    <Preference
        android:persistent="false"
        android:title="Copy FCM Token to clipboard ��"
        android:key="user_pref_debug_copy_fcm"
        app:iconSpaceReserved="false"/>
    <SwitchPreference
        android:persistent="false"
        android:title="@string/setting_switch_debug_ble"
        android:key="user_pref_debug_ble"
        app:iconSpaceReserved="false"/>
    <SwitchPreference
        android:persistent="false"
        android:title="@string/setting_switch_debug_data"
        android:key="user_pref_key_debug_data"
        app:iconSpaceReserved="false"/>
    <SwitchPreference
        android:persistent="false"
