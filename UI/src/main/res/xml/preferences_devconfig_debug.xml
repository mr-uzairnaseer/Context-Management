        android:key="user_pref_key_pioneer_state_override"
        android:defaultValue="@bool/config_feature_default_pioneer_override"
        app:iconSpaceReserved="false"/>
    <ListPreference
        android:persistent="false"
        android:title="@string/setting_feature_compass_title"
        android:key="pref_key_compass_sensor"
