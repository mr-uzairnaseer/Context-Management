        app:iconSpaceReserved="false"/>
    <Preference
        android:persistent="false"
        android:title="Address"
        android:key="pref_address"
        app:iconSpaceReserved="false"/>
    <PreferenceCategory
        android:title="Internal states"
        app:iconSpaceReserved="false">
        <Preference
            android:persistent="false"
            android:title="Tour Upload Tasks"
            android:key="pref_key_tour_upload_tasks"
            android:summary=""
            app:iconSpaceReserved="false"/>
        <Preference
            android:persistent="false"
            android:title="Recorder Storage"
            android:key="pref_key_recorder_storage"
            android:summary=""
            app:iconSpaceReserved="false"/>
